using System;

namespace ConsoleApp2
{
    class Program
    {
        static void Main(string[] args)
        {
            string name = "Королев";
            string surname = "Алексей";
            string temporaryStorage;

            Console.WriteLine($"Данные не верные, перепутанные: Имя {name}, Фамилия {surname}.");

            temporaryStorage = surname;
            Surname = name;
            Name = temporaryStorage;

            Console.WriteLine($"Корректные данные: Имя {name}, Фамилия {surname}");
            Console.ReadKey();
        }
    }
}
