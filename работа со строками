using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp2
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.Write("Как вас зовут?:");
            string Name = Console.ReadLine();
            Console.Write("какой ваш знак зодиака?:");
            string odiacSign = Console.ReadLine();
            Console.Write("сколько вам лет?:");
            int Age =Convert.ToInt32( Console.ReadLine());
            Console.Write("какая ваша проффесия?:");
            string work = Console.ReadLine();
            Console.WriteLine($"Вас зовут {Name}, ваш знак зодиака {odiacSign}, ваш возраст {Age} лет, ваша проффесия {work}");
            Console.ReadKey();
        }
    }
}
