using System;

namespace ConsoleApp2
{
    class Program
    {
        static void Main(string[] args)
        {
            int PicturesInRow = 3;
            int Pictures = 52;
            int FilledRow;
            int ExtraPictures;

            ExtraPictures = Pictures % PicturesInRow;
            FilledRow = Pictures / PicturesInRow;

            Console.WriteLine($"Можно вывести {FilledRow} заполненых рядов. {extraPictures} картинка будет сверх меры.");
            Console.ReadKey();
        }
    }
}
