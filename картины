using System;

namespace ConsoleApp2
{
    class Program
    {
        static void Main(string[] args)
        {
            int picturesInRow = 3;
            int pictures = 52;
            int filledRows;
            int extraPictures;

            extraPictures = pictures % picturesInRow;
            filledRows = pictures / picturesInRow;

            Console.WriteLine($"Можно вывести {filledRows} заполненых рядов. {extraPictures} картинка будет сверх меры.");
            Console.ReadKey();
        }
    }
}
